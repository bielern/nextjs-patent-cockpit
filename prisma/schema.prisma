generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model patents {
  id      Int      @id @default(autoincrement())
  name    String   @db.VarChar(255)
}